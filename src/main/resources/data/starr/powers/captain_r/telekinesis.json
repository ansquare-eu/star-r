{
  "type": "origins:multiple",
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": false,
    "key": {
      "key": "key.origins.secondary_active"
    }
  },
  "telekinesis": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "starr:telekinesis_tick",
      "default_distance": 5
    },
    "falling_action": {
      "type": "starr:telekinesis_end"
    },
    "rising_action": {
      "type": "origins:raycast",
      "distance": 16,
      "block": true,
      "entity": true,
      "shape_type": "visual",
      "fluid_handling": "any",
      "bientity_action": {
        "type": "starr:telekinesis_start"
      },
      "block_action": {
        "type": "starr:telekinesis_block"
      },
      "before_action": {
        "type": "starr:telekinesis_task"
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "increment":{
    "type": "origins:active_self",
    "entity_action": {
      "type": "starr:telekinesis_increment",
      "increment": true,
      "amount": 1
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    },
    "key": {
      "key": "key.origins.increment",
      "continuous": true
    }
  },
  "decrement":{
    "type": "origins:active_self",
    "entity_action": {
      "type": "starr:telekinesis_increment",
      "increment": false,
      "amount": 1
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    },
    "key": {
      "key": "key.origins.decrement",
      "continuous": true
    }
  }
}
