{
  "type": "origins:multiple",
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": false,
    "key": {
      "key": "key.origins.6_active"
    }
  },
  "airwalk": {
      "type": "origins:action_over_time",
      "interval": 1,
      "entity_action": {
        "type": "starr:airwalk",
        "block": "starr:forcefield",
        "delay": 20
      },
      "falling_action": {
        "type": "starr:airwalk_stop"
      },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  }
}
